@if(projects != null && projects.Count > 0)
{
    <section class="stack-box py-0 z-index-99"> 
        <div class="stack-box-contain">
            @for(var i = 0; i < projects.Count; i++)
            {
                var project = projects[i];
                var backgroundClass = "bg-white";
                if(i % 2 == 1)
                {
                    backgroundClass = "bg-base-color-light";
                }
                <!-- start stack item -->
                <div class="stack-item stack-item-0@(i+1) @backgroundClass lg-pt-8 sm-pt-0 lg-pb-8 md-pb-0">
                    <div class="stack-item-wrapper">
                        <div class="container-fluid">
                            <div class="row align-items-center full-screen md-h-auto"> 
                                <div class="col-lg-6 cover-background overflow-visible h-100 md-h-500px" style="background-image: url(@project.Images.First().GetCropUrl("mainSlider"))">
                                    <div class="position-absolute right-0px bottom-minus-1px">
                                        <div class="vertical-title-center">
                                            <div class="title fw-700 fs-15 alt-font text-uppercase text-dark-gray @backgroundClass pt-30px pb-30px ps-10px pe-10px">
                                                <span class="d-inline-block">Naši projekti</span>
                                            </div>
                                        </div>
                                    </div>

                                    @* @if(project.IsActive)
                                    {
                                        <figcaption class="position-absolute tw:rounded-l-2xl bg-base-color right-0px tw:top-[10%] p-45px last-paragraph-no-margin">
                                            <i class="line-icon-Pricing icon-extra-large text-white mb-20px justify-content-center"></i>
                                            <p class="text-white">Cijena od</p>
                                            <h4 class="alt-font fw-700 text-white mb-0 d-block">@project.PriceFrom.ToString("N0") € / m<sup>2</sup></h4>
                                        </figcaption>
                                    } *@
                                </div>
                                <div class="col-lg-6 ps-12 pe-12 xxl-ps-10 xxl-pe-10 xl-pe-8 lg-ps-6 lg-pe-4 md-p-50px sm-ps-30px sm-pe-30px position-relative align-self-center text-md-start text-center"> 
                                    @if(project.IsActive)
                                    {
                                        <div class="mb-15px">
                                            <span class="label tw:bg-green-100 tw:text-green-600 border-radius-26px tw:px-3 tw:py-1">U prodaji</span>
                                        </div>
                                    }
                                    <h1 class="text-dark-gray fs-90 alt-font fw-600 ls-minus-4px mb-10px">@project.Name</h1>
                                    <div class="mb-20px">
                                        <span class="w-30px h-2px fs-15 d-inline-block bg-base-color me-5px align-middle"></span>
                                        <span class="text-base-color fs-20 fw-600 d-inline-block">@project.Subtitle</span>
                                    </div>
                                    <p class="w-95 md-w-100 mb-35px">@Html.Raw(project.ShortDescription)</p>
                                    <a href="@project.ProjectLink?.Url" target="@project.ProjectLink?.Target" class="btn btn-large btn-rounded btn-transparent-base-color btn-hover-animation-switch d-table d-lg-inline-block lg-mb-15px md-mx-auto">
                                        <span>
                                            <span class="btn-text">Saznajte više</span>
                                            <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span>
                                            <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span>
                                        </span> 
                                    </a> 
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- end stack item -->
            }
        </div>
    </section>
}
