@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<HomePage>
@{
	Layout = "Master.cshtml";

    var bannerImages = Model.BannerImages;

    var projects = Model.Descendants<Project>().ToList();
    var contactPage = Model.Children<ContactPage>().FirstOrDefault();
    var projectsPage = Model.Children<Projects>().FirstOrDefault();
}

<section class="p-0 full-screen top-space-margin position-relative overflow-hidden md-h-auto">
    <div class="container-fluid p-0 h-100 position-relative">
        <div class="row h-100 g-0">
            <div class="col-xl-5 col-lg-6 d-flex justify-content-center flex-column ps-10 xxl-ps-5 xl-ps-2 md-ps-0 cover-background tw:overflow-visible position-relative order-2 order-lg-1" style="background-image:url('/images/hero-left-3.png');">
                <div class="border-start border-color-extra-medium-gray ps-60px ms-100px lg-ps-30px lg-ms-70px position-relative tw:z-30 sm-ps-30px sm-pe-30px sm-ms-0 border-0" data-anime='{ "el": "childs", "translateY": [30, 0], "opacity": [0,1], "duration": 600, "delay":0, "staggervalue": 300, "easing": "easeOutQuad" }'>
                    <div class="mb-15px tw:flex tw:flex-col tw:md:flex-row tw:items-center tw:md:gap-2">
                        <div>
                            <i class="fa fa-star tw:text-yellow-500"></i>
                            <i class="fa fa-star tw:text-yellow-500"></i>
                            <i class="fa fa-star tw:text-yellow-500"></i>
                            <i class="fa fa-star tw:text-yellow-500"></i>
                            <i class="fa fa-star tw:text-yellow-500"></i>
                        </div>
                        <span class="alt-font">@Model.BannerSocialProof</span>
                    </div>
                    <h1 class="text-dark-gray tw:text-center tw:md:text-left fs-100 lg-fs-55 fw-600 alt-font outside-box-right-10 xl-outside-box-right-15 ls-minus-4px sm-ls-minus-2px md-me-0">
                        <span>@Html.Raw(Model.BannerTitle)</span>
                    </h1>
                    <div class="fs-24 w-90 mb-35px lg-w-90 sm-w-100 tw:text-center tw:md:text-left">@Html.Raw(Model.BannerSubtitle)</div>
                    <div class="tw:flex tw:flex-col tw:items-center tw:md:flex-row tw:gap-2">
                        <a href="@contactPage?.Url()" class="btn btn-base-color btn-large border-1 btn-round-edge btn-box-shadow me-15px xs-mt-10px xs-mb-10px">Kontaktirajte nas</a>
                        <a href="@projectsPage?.Url()" class="btn btn-large btn-link btn-hover-animation-switch text-dark-gray p-0 ">
                            <span>
                                <span class="btn-text">Naši projekti</span>
                                <span class="btn-icon"><i class="feather icon-feather-arrow-right"></i></span>
                                <span class="btn-icon"><i class="feather icon-feather-arrow-right"></i></span>
                            </span> 
                        </a>
                    </div>
                </div>
            </div>
            <div class="col-xl-7 col-lg-6 position-relative swiper-number-pagination-progress md-h-500px order-1 order-lg-2 md-mb-50px"> 
                <div class="swiper h-100 banner-slider magic-cursor drag-cursor" data-slider-options='{ "slidesPerView": 1, "loop": true, "pagination": { "el": ".swiper-number-line-pagination", "clickable": true }, "navigation": { "nextEl": ".slider-one-slide-next-1", "prevEl": ".slider-one-slide-prev-1" }, "autoplay": { "delay": 7000, "stopOnLastSlide": true, "disableOnInteraction": false },"keyboard": { "enabled": true, "onlyInViewport": true }, "effect": "fade" }' data-swiper-number-pagination-progress="true">
                    <div class="swiper-wrapper">
                        @foreach(var bannerImage in bannerImages)
                        {
                            <!-- start slider item -->
                            <div class="swiper-slide">
                                <div class="position-absolute left-0px top-0px w-100 h-100 cover-background background-position-center-top" style="background-image:url('@bannerImage.GetCropUrl("bannerImage")');"></div>
                            </div>
                            <!-- end slider item -->
                        }
                    </div>
                    <!-- start slider pagination -->
                    <div  class="swiper-pagination swiper-pagination-clickable swiper-pagination-bullets d-block d-sm-none"></div>
                    <!-- end slider pagination --> 
                    <!-- start slider navigation -->
                    <!-- <div class="slider-one-slide-prev-1 icon-very-small text-white swiper-button-prev slider-navigation-style-06 d-none d-sm-inline-block"><i class="line-icon-Arrow-OutLeft icon-extra-large"></i></div>
                        <div class="slider-one-slide-next-1 icon-very-small text-white swiper-button-next slider-navigation-style-06 d-none d-sm-inline-block"><i class="line-icon-Arrow-OutRight icon-extra-large"></i></div> -->
                    <!-- end slider navigation -->
                </div>
                <div class="tw:h-full tw:w-full tw:absolute tw:top-0 tw:left-0 tw:bg-gradient-to-r tw:from-white/40 tw:to-transparent tw:z-20"></div>
                <!-- start slider pagination -->
                <div class="swiper-pagination-wrapper d-none d-lg-flex align-items-center justify-content-center position-absolute bottom-40px md-bottom-30px sm-bottom-20px left-minus-45 md-left-30px sm-left-20px z-index-9">
                    <div class="number-prev fs-14 fw-600 text-dark-gray"></div>
                    <div class="swiper-pagination-progress bg-extra-medium-gray">
                        <span class="swiper-progress dark"></span>
                    </div>
                    <div class="number-next fs-14 fw-600 text-dark-gray"></div>    
                </div>
                <!-- end slider pagination -->
            </div>
        </div>
    </div>
</section>

